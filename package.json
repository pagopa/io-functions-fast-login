{
  "name": "io-functions-fast-login",
  "version": "1.0.0",
  "description": "Fast Login functions",
  "main": "./dist/main.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "shx rm -rf dist && npm run generate",
    "postbuild": "dependency-check package.json --no-dev --missing ./dist/**/*.js",
    "build": "tsc",
    "watch": "tsc -w",
    "extensions:install": "func extensions install",
    "start": "dotenv -e .env func start --javascript",
    "predev": "npm-run-all build",
    "dev": "npm-run-all --parallel start watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx --cache",
    "lint-api": "find ./api -name '*.yaml' -exec swagger-cli validate {} \\;",
    "api:bundle": "bundle-api-spec -i api/internal.yaml.template -o api/internal.yaml -V $npm_package_version",
    "pregenerate": "npm-run-all api:bundle*",
    "generate": "npm-run-all generate:*",
    "generate:definitions-internal": "rimraf ./src/generated/definitions/internal && shx mkdir -p ./src/generated/definitions/internal && gen-api-models --api-spec ./api/internal.yaml --no-strict --out-dir ./src/generated/definitions/internal ",
    "dist:modules": "modclean -r -n default:safe && yarn install --production",
    "predeploy": "npm-run-all build dist:*",
    "preversion": "auto-changelog  --config .auto-changelog.json --unreleased --commit-limit false --stdout --template preview.hbs",
    "version": "npm-run-all bump:*",
    "bump:api": "npm-run-all api:* && git add api/*.yaml",
    "bump:changelog": "auto-changelog -p --config .auto-changelog.json --unreleased && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@pagopa/eslint-config": "^3.0.0",
    "@types/jest": "^24.0.15",
    "@types/node": "~18.13.0",
    "dependency-check": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^8.28.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^24.8.0",
    "modclean": "^3.0.0-beta.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "shx": "^0.3.2",
    "swagger-cli": "^4.0.4",
    "ts-jest": "^24.3.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@azure/cosmos": "^3.17.3",
    "@azure/functions": "^3.5.1",
    "@pagopa/handler-kit": "^1.1.0",
    "@pagopa/handler-kit-azure-func": "^1.2.0",
    "@pagopa/io-functions-commons": "^26.2.1",
    "@pagopa/openapi-codegen-ts": "^12.2.0",
    "@pagopa/ts-commons": "^10.14.2",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.19",
    "io-ts-types": "^0.5.19"
  }
}
